{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "slot",
  "homepage": "https://slot-ui.com",
  "items": [
    {
      "name": "theme",
      "type": "registry:file",
      "description": "Slot UI design tokens, dark mode, corner shape utility, and base styles.",
      "docs": "Import slot-ui styles in your CSS file (after @import \"tailwindcss\"):\n\n  @import \"~/styles/slot-ui.css\";\n",
      "css": {
        "@import \"../styles/slot-ui.css\"": {}
      },
      "files": [
        {
          "path": "styles/slot-ui.css",
          "type": "registry:file",
          "target": "~/styles/slot-ui.css"
        }
      ]
    },
    {
      "name": "utils",
      "type": "registry:item",
      "description": "Utility functions for Slot UI components.",
      "dependencies": [
        "tailwind-merge",
        "clsx"
      ],
      "files": [
        {
          "path": "lib/utils.ts",
          "type": "registry:file",
          "target": "~/lib/utils.ts"
        }
      ]
    },
    {
      "name": "ui",
      "type": "registry:item",
      "title": "All Components",
      "description": "Install all Slot UI components.",
